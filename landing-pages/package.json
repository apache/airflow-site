{
  "name": "apache-airflow-site",
  "version": "1.0.0",
  "description": "Apache Airflow website",
  "repository": "apache/airflow-site",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "$npm_execpath run build:webpack && $npm_execpath run build:hugo",
    "build:hugo": "cross-env HUGO_ENV=production hugo -d ../dist -s site",
    "build:hugo:preview": "$npm_execpath run build:hugo -- -D -F",
    "build:preview": "$npm_execpath run build:webpack && $npm_execpath run build:hugo:preview",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "index": "node ./create-index.js",
    "lint": "run-p lint:**",
    "lint:css": "stylelint \"site/assets/scss/**/*.scss\"",
    "lint:js": "eslint .",
    "prebuild": "rimraf dist && $npm_execpath run index",
    "preview": "run-p preview:**",
    "preview:hugo": "$npm_execpath run start:hugo -- -D -F",
    "preview:webpack": "$npm_execpath run start:webpack",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -w",
    "start:webpack": "webpack serve --config webpack.dev.js --hot"
  },
  "dependencies": {
    "lodash": "^4.17.23",
    "lunr": "^2.3.9",
    "p5": "^2.2.0",
    "parser-front-matter": "^1.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/register": "^7.28.6",
    "@eslint/js": "^9.39.2",
    "assets-webpack-plugin": "^7.1.1",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.3",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "exports-loader": "^5.0.0",
    "fancy-log": "^2.0.0",
    "file-loader": "^6.2.0",
    "globals": "^17.2.0",
    "imports-loader": "^5.0.0",
    "mini-css-extract-plugin": "^2.10.0",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^2.0.1",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^11.1.2",
    "rimraf": "^6.1.2",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^17.0.0",
    "stylelint-config-recommended": "^18.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3",
    "webpack-merge": "^6.0.1",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard-scss"
    ],
    "rules": {
      "alpha-value-notation": null,
      "at-rule-empty-line-before": null,
      "color-function-alias-notation": null,
      "color-function-notation": null,
      "color-hex-length": null,
      "declaration-block-no-redundant-longhand-properties": null,
      "declaration-block-single-line-max-declarations": null,
      "declaration-empty-line-before": null,
      "declaration-property-value-no-unknown": null,
      "import-notation": null,
      "length-zero-no-unit": null,
      "media-feature-range-notation": null,
      "media-query-no-invalid": null,
      "property-no-deprecated": null,
      "property-no-vendor-prefix": null,
      "rule-empty-line-before": null,
      "scss/at-extend-no-missing-placeholder": null,
      "scss/at-rule-no-unknown": null,
      "scss/dollar-variable-empty-line-before": null,
      "scss/dollar-variable-no-missing-interpolation": null,
      "scss/dollar-variable-pattern": null,
      "scss/double-slash-comment-empty-line-before": null,
      "scss/double-slash-comment-whitespace-inside": null,
      "scss/function-no-unknown": null,
      "scss/no-global-function-names": null,
      "selector-class-pattern": null,
      "shorthand-property-no-redundant-values": null,
      "value-keyword-case": null,
      "value-no-vendor-prefix": null
    }
  }
}
